enc = 

r = Zmod(M)
a = 1
b = 0
for i in trange(100000):
    a = (a * A) % M
    b = ((b * A) + B) % M
    sqrt_disc = (r(b) ^ 2 + 4 * r(n) * r(a)).sqrt()
    p1 = (-r(b) + sqrt_disc) / r(2) / r(a)
    p2 = (-r(b) - sqrt_disc) / r(2) / r(a)
    try:
        p1 = int(p1)
        if n % p1 == 0:
            p, q = p1, (a * p1 + b) % M
            print(f"p = {p}\nq = {q}")
            break
        p2 = int(p2)
        if n % p2 == 0:
            p, q = p2, (a * p2 + b) % M
            print(f"p = {p}\nq = {q}")
            break
    except ValueError:
        continue

phi = (p-1) * (q-1)
d = Zmod(phi)(65537) ^ -1

print(int(pow(enc, d, n)).to_bytes(127, byteorder="big").replace(b"\x00", b""))